<intelxdk:version value="1.1"/>
<intelxdk:cordova-cli version="6.2.0"/>
<name>Yoga_Positions</name>
<description>A simple multi-page app that uses the App Framework UI Library.</description>
<author>Intel XDK</author>
<content src="index.html"/>
<access origin="http://debug-software.intel.com/*"/>
<access origin="https://debug-software.intel.com/*"/>
<access origin="http://en.wikipedia.org/*"/>
<access origin="http://www.yogajournal.com/*"/>
